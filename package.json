{
  "name": "git-lfs-file-locker",
  "displayName": "Git LFS File Locker",
  "description": "Adds a context menu item to lock/unlock files with Git LFS.",
  "version": "0.6.3",
  "publisher": "TheArche",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#6C88C4",
    "theme": "dark"
  },
  "readme": "This extension adds a context menu item to lock/unlock files with Git LFS.",
  "homepage": "https://github.thearche.de/",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/thearche/git-lfs-file-locker"
  },
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "SCM Providers",
    "Other"
  ],
  "tags": [
    "git",
    "git-lfs",
    "lfs",
    "file-locking"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:.git/lfs/config"
  ],
  "contributes": {
    "commands": [
      {
        "command": "git-lfs-file-locker.lockFile",
        "title": "Git LFS: Lock File",
        "category": "Git LFS"
      },
      {
        "command": "git-lfs-file-locker.unlockFile",
        "title": "Git LFS: Unlock File",
        "category": "Git LFS"
      },
      {
        "command": "git-lfs-file-locker.showLocks",
        "title": "Git LFS: Show Active Locks in Editor",
        "category": "Git LFS",
        "icon": "$(lock)"
      },
      {
        "command": "git-lfs-file-locker.refreshLocks",
        "title": "Refresh",
        "category": "Git LFS",
        "icon": "$(refresh)"
      },
      {
        "command": "git-lfs-file-locker.unlockFileFromView",
        "title": "Unlock",
        "category": "Git LFS",
        "icon": "$(unlock)"
      },
      {
        "command": "git-lfs-file-locker.revealFileFromView",
        "title": "Reveal in Explorer",
        "category": "Git LFS",
        "icon": "$(go-to-file)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceScheme == 'file'",
          "command": "git-lfs-file-locker.lockFile",
          "group": "9_git@1"
        },
        {
          "when": "resourceScheme == 'file'",
          "command": "git-lfs-file-locker.unlockFile",
          "group": "9_git@2"
        }
      ],
      "view/title": [
        {
          "command": "git-lfs-file-locker.refreshLocks",
          "when": "view == gitLfsLocksView",
          "group": "navigation"
        },
        {
          "command": "git-lfs-file-locker.showLocks",
          "when": "view == gitLfsLocksView",
          "group": "navigation@1"
        }
      ],
      "view/item/context": [
        {
          "command": "git-lfs-file-locker.unlockFileFromView",
          "when": "view == gitLfsLocksView && viewItem == lfsLock",
          "group": "inline"
        },
        {
          "command": "git-lfs-file-locker.revealFileFromView",
          "when": "view == gitLfsLocksView && viewItem == lfsLock",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "git-lfs-locks-container",
          "title": "Git LFS",
          "icon": "$(lock)"
        }
      ]
    },
    "views": {
      "git-lfs-locks-container": [
        {
          "id": "gitLfsLocksView",
          "name": "Active Locks",
          "type": "tree",
          "icon": "$(lock)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "1.108.1",
    "@types/node": "25.1.0",
    "typescript": "5.9.3",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "eslint": "9.39.2"
  }
}